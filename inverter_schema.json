{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "supplier_name": {
      "type": "string",
      "description": "Name of the supplier/manufacturer"
    },
    "product_code": {
      "type": "string",
      "description": "Product code or model number of the inverter"
    },
    "description": {
      "type": "string",
      "description": "Description of the inverter product"
    },
    "startup_voltage": {
      "type": "string",
      "description": "Startup voltage setting in volts (e.g., '150V')"
    },
    "firmware_version": {
      "type": "string",
      "description": "Firmware version number (e.g., 'v2.1.4')"
    },
    "valid_from": {
      "type": "string",
      "format": "date-time",
      "description": "Start date and time when settings were active"
    },
    "valid_to": {
      "type": "string",
      "format": "date-time",
      "description": "End date and time when settings were active"
    },
    "unit_price": {
      "type": "number",
      "description": "Price per unit"
    },
    "currency": {
      "type": "string",
      "description": "Currency code for the price"
    },
    "effective_date": {
      "type": "string",
      "format": "date",
      "description": "Date when the pricing or configuration became effective"
    }
  },
  "required": [
    "supplier_name",
    "product_code",
    "description",
    "startup_voltage",
    "firmware_version",
    "valid_from",
    "valid_to"
  ],
  "additionalProperties": false
} 